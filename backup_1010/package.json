{
  "engines": {
    "node": ">=22 <23"
  },
  "scripts": {
    "dev": "next dev",
    "dev:timeout": "node scripts/start-server-with-timeout.js",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "db:fmt": "prisma format",
    "db:val": "prisma validate",
    "db:gen": "prisma generate",
    "db:up": "docker compose up -d postgres",
    "db:down": "docker compose down",
    "db:reset": "prisma migrate reset --force",
    "db:migrate": "prisma migrate dev --name init_pg",
    "check": "npm run db:fmt && npm run db:val && npm run db:gen && npm run lint && npm run typecheck",
    "check-hydration": "node scripts/check-hydration.js",
    "check-hydration:dev": "NODE_ENV=development node scripts/check-hydration.js",
    "check-hydration:prod": "NODE_ENV=production node scripts/check-hydration.js",
    "init:logging": "node scripts/init-logging.js",
    "monitor": "echo 'Open http://localhost:3000/monitor in your browser'"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.16.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-query": "^5.90.2",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.22",
    "jose": "^6.1.0",
    "lucide-react": "^0.543.0",
    "next": "15.5.2",
    "nodemailer": "^6.10.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-intersection-observer": "^9.16.0",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "tough-cookie": "^6.0.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "postcss": "^8.5.6",
    "prisma": "^6.16.2",
    "puppeteer": "^24.23.0",
    "tailwindcss": "^4.1.13",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs"
  }
}
